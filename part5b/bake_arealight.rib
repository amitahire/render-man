			AttributeEnd

			AttributeBegin
				Translate -10 0 -70
				Rotate -11.46 0 0 1
				Scale 3 110 3
				Color [0.8 0.2 0.8]
				Rotate 90 1 0 0
				Cylinder 1 -1 1 360
